<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="hibernateorder.PurchaseOrderLine" table="PURCHASEORDERLINE">
        <id column="LINENO" name="lineNo">
            <generator class="increment"/>
        </id>
        <property column="PARTNAME" name="partName"/>
        <property column="AMOUNT" name="amount"/>
        <property column="ORDERID" name="orderid"/>
        </class>
</hibernate-mapping>
